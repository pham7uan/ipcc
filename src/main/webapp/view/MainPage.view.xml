<mvc:View
        controllerName="sap.ui.ipcc.wt.controller.MainPage"
        xmlns="sap.m"
        xmlns:mvc="sap.ui.core.mvc">
    <Page title="IPCC Outbound" enableScrolling="true" class="sapUiContentPadding">
        <headerContent>

        </headerContent>
        <content>
            <VBox class="sapUiSmallMargin">
                <Button
                        width="100%"
                        text="Open Voice Mail List Page"
