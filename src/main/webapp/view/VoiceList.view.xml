<mvc:View
        xmlns="sap.m"
        xmlns:mvc="sap.ui.core.mvc">
    <List
            headerText="{i18n>voiceListTitle}"
            class="sapUiResponsiveMargin"
            width="auto"
            items="{voice>/}">
        <items>
            <ObjectListItem
                    title="{voice>id} : {voice>customer_name}"

            />
        </items>
    </List>
</mvc:View>