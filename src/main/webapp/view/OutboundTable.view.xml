<mvc:View
        controllerName="sap.ui.ipcc.wt.controller.OutboundTable"
        xmlns:html="http://www.w3.org/1999/xhtml"
        xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
        xmlns="sap.ui.table"
        xmlns:mvc="sap.ui.core.mvc"
        xmlns:u="sap.ui.unified"
        xmlns:l="sap.ui.layout"
        xmlns:c="sap.ui.core"
        xmlns:m="sap.m">

    <m:Toolbar>
        <m:Title text="{i18n>outboundListTitle}"/>
        <m:ToolbarSpacer/>
        <m:Button
                id="btnDownload"
                icon="sap-icon://download"
                tooltip="Download sample excel file"
                text="Get Import Form"
                press="handleDownloadPress"/>
    </m:Toolbar>

    <m:HBox class="sapUiSmallMargin">
        <m:Text text="Select Table" textAlign="Center" class="sapUiSmallMargin"/>
        <m:ComboBox
                class="sapUiSmallMarginEnd"
                id="combobox"
                selectedKey="0"
                items="{
					path: '/OutboundTables',
					sorter: { path: 'name' }
				}">
            <c:Item key="{id}" text="{name}" />
        </m:ComboBox>

        <u:FileUploader
                class="sapUiSmallMarginEnd"
                id="fileUploader"
                name="file"
                uploadUrl="/api/birthday/import/"
                width="400px"
                tooltip="Import your file to the database"
                uploadComplete="handleUploadComplete"
                change="handleValueChange"
                typeMissmatch="handleTypeMissmatch"
                style="Emphasized"
                fileType="xlsx,xls"
                placeholder="Choose a file for Import...">
            <u:parameters>
                <u:FileUploaderParameter
                        name="param1"
                        value="Foo"/>
                <u:FileUploaderParameter
                        name="param2"
                        value="bar"/>
            </u:parameters>
            <u:headerParameters>
                <u:FileUploaderParameter
                        name="headerParam1"
                        value="Foo"/>
                <u:FileUploaderParameter
                        name="headerParam2"
                        value="bar"/>
            </u:headerParameters>
        </u:FileUploader>
        <m:Button
                class="sapUiSmallMarginEnd"
                id="btnUpload"
                icon="sap-icon://upload"
                tooltip="Import data from excel file"
                text="Import File"
                press="handleUploadPress"/>
        <m:Button
                text="Export to Excel"
                icon="sap-icon://excel-attachment"
                tooltip="Export table data to excel file"
                press="exportFile"/>
    </m:HBox>

    <m:TextArea id="result" growing="true" editable="false" value="" textAlign="Left" width="100%">

    </m:TextArea>

</mvc:View>